  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "start": "npm run build-client & node index.js",
    "dev": "concurrently --kill-others \"npm run dev-client\" \"npm run start-redis\"\"nodemon index.js\" \"cross-env ENV='development'\"",
